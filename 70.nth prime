public class Prime{
  public static void main(String[] args){
    int a=1;
    int b=10;
    for(int i=a;i<=b;i++){
    boolean isPrime=true;
    for(int j=2;j<=i/2;j++){
    if(i%j==0){
        isPrime=false;
        break;
    }
    }
    if(isPrime && i>1)
    {
    System.out.println(i);
    }
    }
  }
}
    
