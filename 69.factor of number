public class FactorsOfNumber {
    public static void main(String[] args) {
        int num = 100, N = 4, count = 0;
        for (int i = 1; i <= num; i++) {
            if (num % i == 0) {
                count++;
                if (count == N) 
                System.out.println(i);
            }
        }
        System.out.println("Number of factors = " + count);
    }
}
